  <children>
    <MenuBar VBox.vgrow="NEVER" />
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <TextArea fx:id="txtLog" layoutX="14.0" layoutY="119.0" prefHeight="267.0" prefWidth="614.0" editable="false" />
            <Button fx:id="btnIniciar" onAction="#iniciar" layoutX="28.0" layoutY="66.0" mnemonicParsing="false" text="Iniciar" />
            <Button fx:id="btnImprimir" onAction="#imprimir" layoutX="426.0" layoutY="66.0" mnemonicParsing="false" text="Imprimir" disable="true" />
            <Button fx:id="btnSalir" onAction="#salir" layoutX="529.0" layoutY="66.0" mnemonicParsing="false" text="Salir" />
         </children>
    </AnchorPane>
  </children>
</VBox>
